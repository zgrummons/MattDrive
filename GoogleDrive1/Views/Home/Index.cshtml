@{
    ViewData["Title"] = "Home Page";
}
@foreach (var dir in DirectoryModel.Instance.Parents)
{
    @Html.ActionLink(dir.Name, "Index", "Home", new { dir.Id }) @Html.Raw(" / ")
}
<br />&mdash;&mdash;&mdash;<br/>
@foreach (Google.Apis.Drive.v3.Data.File file in Model.Files)
{
    @Html.ActionLink(file.Name, "Index", "Home", new { file.Id })<br />
}
